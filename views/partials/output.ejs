<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        tr,td,th{
            border: 1px solid black ;
           
        }
        table{
            border-collapse: collapse;
            text-align: center;
            width: 90%;
            margin: auto;
        }
        .query{
            /* background-color: rgb(185, 105, 105); */
            color: brown;
            padding: 10px;
            border-radius: 5px;
        }
    </style>
</head>
<body>
    <p style="text-align: center; margin-bottom: 60px;">QUERY : <span class="query"><%=sql2 %></span></p>
    <table>
        <tr>
            <% Object.keys(result[0]).forEach(function(column){ %>
                <th><%= column %></th>
            <% }) %>
        </tr>
        <tr>
            <% result.forEach(function(row){ %>
                <tr>
                    <% Object.values(row).forEach(function(value){ %>
                        <td><%= value %></td>
                    <% }) %>
                </tr>
            <% }) %>
        </tr>
    </table>
</body>
</html>